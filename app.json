{
  "expo": {
    "name": "life_pharmacy",
    "slug": "life_pharmacy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_phamacy.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pharmacy.ios",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo_phamacy.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sandeepdhobi.life_pharmacy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_phamacy.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d47047de-17c8-4810-aa89-64df46ec1cde"
      }
    }
  }
}
